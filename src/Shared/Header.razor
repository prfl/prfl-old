@inject Services.StateContainer StateContainer

<div class="container rounded pb-2 bg-light">

<div class="mt-3 text-center">
    <img src="/assets/logo/person-circle.svg" />
</div>
<div class="mt-1 text-center">
    <div class="text-strong"><strong>@@<span>@Username</span></strong></div>
</div>



</div>

@code {
    public string Username => StateContainer != null ? $"{StateContainer.Username}" : "";
}